{
  "name": "keygen-customer-portal-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "npm run css:build-and-copy",
    "build": "ng build",
    "prestart": "npm run css:build-and-copy",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint": "ng lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "debug:log": "node debug-logger.js --debug",
    "debug:log:dry": "node debug-logger.js --debug --dry-run",
    "debug:rollback": "node debug-logger.js --rollback",
    "debug:rollback:dry": "node debug-logger.js --rollback --dry-run",
    "docs:build": "compodoc -p tsconfig.app.json",
    "docs:serve": "compodoc -p tsconfig.app.json -s",
    "arho": "node arho.js",
    "css:build": "node_modules/.bin/tailwindcss -i src/styles.css -o dist/styles.css --watch",
    "css:build:prod": "node_modules/.bin/tailwindcss -i src/styles.css -o dist/styles.css --minify",
    "css:watch": "node_modules/.bin/tailwindcss -i src/styles.css -o dist/styles.css --watch",
    "css:generate": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-generated.css",
    "css:generate:min": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-generated.css --minify",
    "css:generate:watch": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-generated.css --watch",
    "css:purge": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-purged.css --content 'src/**/*.{html,ts}'",
    "css:purge:min": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-purged.css --content 'src/**/*.{html,ts}' --minify",
    "css:purge:watch": "node_modules/.bin/tailwindcss -i src/tailwind-only.css -o dist/tailwind-purged.css --content 'src/**/*.{html,ts}' --watch",
    "css:build-and-copy": "npm run css:purge:min && cp dist/tailwind-purged.css src/tailwind-generated.css",
    "generate-css": "node generate-css.js",
    "hex-ddd-analyze": "hex-ddd-analyzer analyze \"src/**/*.ts\" --config ./hex-ddd.config.json --output console --severity error",
    "hex-ddd-analyze:all": "hex-ddd-analyzer analyze \"src/**/*.ts\" --output console --severity info",
    "hex-ddd-analyze:json": "hex-ddd-analyzer analyze \"src/**/*.ts\" --output json --severity info",
    "hex-ddd-analyze:summary": "hex-ddd-analyzer analyze \"src/**/*.ts\" --output summary --severity warning",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:headed": "playwright test --headed",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:mobile": "playwright test --project='Mobile Chrome'",
    "e2e:auth": "playwright test e2e/tests/auth-debug.spec.ts --config playwright-auth.config.ts",
    "e2e:protected": "playwright test e2e/tests/protected-routes.spec.ts",
    "e2e:setup": "playwright install",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen http://localhost:4200"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --config eslint.config.js --fix --max-warnings 0 --",
      "prettier --write --"
    ],
    "src/**/*.html": [
      "prettier --write --"
    ],
    "src/styles.css": [
      "stylelint --fix --allow-empty-input"
    ],
    "src/tailwind-only.css": [
      "stylelint --fix --allow-empty-input"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.26.0",
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.6",
    "@angular/router": "^21.0.0",
    "@micro-sentry/angular": "^7.2.0",
    "@ng-icons/core": "^31.4.0",
    "@ng-icons/lucide": ">=32.0.0 <34.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-angular": "^21.0.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-class-names": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "heroicons": "^2.2.0",
    "hex-ddd-analyzer": "file:/Users/password9090/MEGA/tsPro",
    "lucide-angular": "^0.562.0",
    "ngx-sonner": "^3.1.0",
    "rxjs": "~7.8.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.3.0",
    "tw-animate-css": "^1.4.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "angular-eslint": "21.1.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.47.0",
    "vitest": "^4.0.8"
  }
}
